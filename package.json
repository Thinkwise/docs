{
  "scripts": {
    "examples": "cd website && docusaurus-examples",
    "start": "cd website && docusaurus-start",
    "build": "cd website && docusaurus-build",
    "publish-gh-pages": "cd website && docusaurus-publish",
    "write-translations": "cd website && docusaurus-write-translations",
    "version": "cd website && docusaurus-version",
    "rename-version": "cd website && docusaurus-rename-version",
    "octo:pack": "node ./scripts/octopack.js",
    "octo:push": "node ./scripts/octopush.js",
    "ci:build": "npm-run-all build",
    "ci:publish": "npm-run-all ci:build octo:pack \"octo:push -- --octopusServer={1} --octopusApiKey={2}\" --"
  },
  "devDependencies": {
    "@octopusdeploy/octojs": "0.1.3",
    "archiver": "3.0.0",
    "cross-env": "5.2.0",
    "docusaurus": "1.7.2",
    "glob": "7.1.3",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "yargs": "13.1.0"
  },
  "dependencies": {
    "algoliasearch": "^3.29.0"
  }
}
